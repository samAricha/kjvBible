<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="teka.mobile.kjvbiblejava">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bible_ic_laucher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/bible_ic_laucher"
        android:supportsRtl="true"
        android:theme="@style/Theme.KjvBiblejava">
        <activity
            android:name="teka.mobile.kjvbiblejava.Activities.TestActivity"
            android:exported="false">

        </activity>
        <activity
            android:name="teka.mobile.kjvbiblejava.Activities.StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="teka.mobile.kjvbiblejava.Activities.ChapterActivity"
            android:exported="false" />
    </application>

</manifest>